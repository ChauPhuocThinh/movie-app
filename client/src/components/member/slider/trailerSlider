import "slick-carousel/slick/slick.css";
import "slick-carousel/slick/slick-theme.css";
import "./slider.scss"
import Slider from "react-slick";
export default function TrailerSlider(props) {
    var settings = {
      dots: true,
      infinite: false,
      speed: 500,
      slidesToShow: 3,
      slidesToScroll: 3,
    };
    return (
      <Slider {...settings} className='trailer-slider'>
        <div>
            <img src={`https://img.youtube.com/viDkHZiAoKITI/hqdefault.jpg`} alt='trailer'/>
        </div>
      </Slider>
    );
  }